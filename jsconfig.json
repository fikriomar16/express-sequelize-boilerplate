{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"outDir": "dist",
		"esModuleInterop": true,
		"lib": ["es6"],
		"moduleResolution": "node",
		"strict": true,
		"noEmitOnError": true,
		"noImplicitAny": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"paths": {
			"#*": ["./src/*"]
		}
	}
}
